@article{gueymardReviewValidationMethodologies2014,
  title = {A Review of Validation Methodologies and Statistical Performance Indicators for Modeled Solar Radiation Data: {{Towards}} a Better Bankability of Solar Projects},
  author = {Gueymard, Christian A},
