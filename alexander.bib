@article{mcelroyModifiedModelbasedSeasonal2008,
  title = {A {{Modified Model-based Seasonal Adjustment}} That {{Reduces Spectral Troughs}} and {{Negative Seasonal Correlation}}},
  author = {McElroy, Tucker S},
